# MCP server endpoint (default: http://localhost:3000/mcp)
MCP_SERVER_ENDPOINT=http://localhost:3000/mcp

# Send Authorization header when true; omit when false (default: false)
AUTHORIZATION_ENABLED=false

# Token sent as Bearer when AUTHORIZATION_ENABLED is true. Must match the authorizer (e.g. MANAGEMENT_API_KEY) when the API has auth enabled.
MCP_AUTH_TOKEN=good_access_token
